<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Home\MyPractoBundle\Entity\TestLabOrders" table="test_lab_orders">
    <indexes>
      <index name="idx_test_lab_id_test_expense_orders" columns="test_lab_id"/>
      <index name="idx_test_expense_id_test_expense_orders" columns="test_expense_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="practiceSettingsId" type="integer" column="practice_settings_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="patientId" type="integer" column="patient_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="testExpense" target-entity="TestExpenses" fetch="LAZY">
      <join-columns>
        <join-column name="test_expense_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="testLab" target-entity="TestLabs" fetch="LAZY">
      <join-columns>
        <join-column name="test_lab_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
